{
    "title": "Wasabi S3 Operator Dashboard",
    "tags": ["wasabi", "s3", "operator", "kubernetes"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Reconciliation Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(wasabi_s3_operator_reconcile_total[5m])",
            "legendFormat": "{{kind}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Reconciliations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Reconciliation Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(wasabi_s3_operator_reconcile_duration_seconds_bucket[5m]))",
            "legendFormat": "{{kind}} - p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(wasabi_s3_operator_reconcile_duration_seconds_bucket[5m]))",
            "legendFormat": "{{kind}} - p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Resource Status",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum by (kind, status) (rate(wasabi_s3_operator_resource_status_total[5m]))",
            "legendFormat": "{{kind}} - {{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "rate(wasabi_s3_operator_error_total[5m])",
            "legendFormat": "{{kind}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Bucket Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "rate(wasabi_s3_operator_bucket_operations_total[5m])",
            "legendFormat": "{{operation}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Provider Connectivity",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(wasabi_s3_operator_provider_connectivity_total{status=\"connected\"}[5m]))",
            "refId": "A",
            "format": "table"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 7,
        "title": "API Call Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(wasabi_s3_operator_api_call_total[5m])",
            "legendFormat": "{{api_type}} - {{operation}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "API Calls/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "API Call Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(wasabi_s3_operator_api_call_duration_seconds_bucket[5m]))",
            "legendFormat": "{{api_type}} - {{operation}} - p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Configuration Drift Detected",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(wasabi_s3_operator_drift_detected_total[5m]))",
            "refId": "A",
            "format": "table"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 10,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(wasabi_s3_operator_rate_limit_hits_total[5m])",
            "legendFormat": "{{api_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Rate Limit Hits/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
}

